# Directories
alias dev='cd ~/Development'
alias web='cd ~/Development/websites'
alias gigs='cd ~/Development/websites/laravel-gigs'
alias crm='cd ~/Development/websites/laravel-crm-v2'
alias rsvp='cd ~/Development/websites/laravel-rsvp'
# Sail
alias sail='bash vendor/bin/sail'
alias sup='sail up -d'
alias sdown='sail down'
alias sart='sail artisan'
alias scomp='sail composer'
alias sfresh='sail artisan migrate:fresh --seed'
alias stest='sail artisan test'
# Valet
alias art='php artisan'
alias comp='php composer'
alias fresh='php artisan migrate:fresh --seed'
alias test='php artisan test'
# Git
alias commit='git add . && git commit -m'
alias wip='commit "WIP"'
